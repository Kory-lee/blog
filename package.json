{
  "name": "kory.blog",
  "version": "1.0.0",
  "author": "Kory-lee",
  "description": "Write once,debug everywhere",
  "main": "index.js",
  "repository": "https://github.com/Kory-lee/myBlog.git",
  "license": "MIT",
  "scripts": {
    "dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "update": "yarn upgrade-interactive --latest",
    "genlog": "conventional-changelog -p angular -i .github/CHANGELOG.md -s",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2",
    "commit": "git add . && git-cz",
    "eslint-ext": "eslint --ext .js,.vue ./",
    "eslint-fix": "eslint --fix --ext .js,.vue ./",
    "push": "yarn commit && git push"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix --ext .js,.vue ./",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@vuepress/plugin-register-components": "^1.5.3",
    "commitizen": "^4.1.2",
    "conventional-changelog": "^3.1.21",
    "conventional-changelog-cli": "^2.0.34",
    "cz-customizable": "^6.3.0",
    "vue-cli-plugin-vue-next": "~0.1.3",
    "vuepress": "1.5.3",
    "vuepress-plugin-flowchart": "^1.4.3",
    "vuepress-plugin-smooth-scroll": "0.0.9",
    "vuepress-theme-reco": "1.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {}
}
